<!-- Binding for JEP-0092: Software Version -->
<binding>
	<namespace uri="jabber:iq:version" default="elements"/>
	<mapping name="query" ns="jabber:iq:version" 
		class="com.echomine.jabber.packet.VersionIQPacket">
  		<value name="name" field="name" usage="optional"/>
  		<value name="version" field="version" usage="optional"/>
  		<value name="os" field="os" usage="optional"/>
  	</mapping>
</binding>
