<!-- Binding for JEP-0077: In-bound registration -->
<binding>
    <namespace uri="jabber:iq:register" default="elements"/>
    <mapping name="query" ns="jabber:iq:register" 
	class="com.echomine.jabber.compat.packet.RegisterIQPacket" ordered='false' >
	<value style="element" name="username" field="username" usage="optional" />
	<value style="element" name="password" field="password" usage="optional" />
	<value style="element" name="instructions" field="instructions" usage="optional" />
	<value style="element" name="key" field="key" usage="optional" />
	<value style="element" name="email" field="email" usage="optional" />
	<value style="element" name="registered" field="registered" usage="optional" />
    </mapping>
</binding>
